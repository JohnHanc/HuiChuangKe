        </activity>
        <!--基类 activity-->
        <activity android:name=".activity.MineActivity"/>
        <activity android:name=".activity.DuttActivity"/>
        <activity android:name=".activity.DynamicActivity"/>
        <activity android:name=".activity.FindProjectActivity"/>
        <activity android:name=".activity.FindPersonActivity"/>
        <activity android:name=".activity.BaseActivity"/>

    </application>

</manifest>